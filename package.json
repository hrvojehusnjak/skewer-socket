{
  "name": "skewer-socket",
  "version": "1.0.0",
  "description": "Stream text similarity comparisons via sockets",
  "main": "./dist/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "start:watch": "nodemon",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hrc099/skewer-socket.git"
  },
  "author": "Hrvoje Husnjak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hrc099/skewer-socket/issues"
  },
  "homepage": "https://github.com/hrc099/skewer-socket#readme",
  "dependencies": {
    "@types/socket.io": "^2.1.2",
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "jaccard": "^0.4.0",
    "socket.io": "^2.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/node": "^12.7.5",
    "eslint": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.19.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
